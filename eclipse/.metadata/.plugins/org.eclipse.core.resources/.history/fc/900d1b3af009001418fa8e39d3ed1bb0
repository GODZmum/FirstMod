package com.GODZmum.letsModReboot.block;

import net.minecraft.creativetab.CreativeTabs;

public class BlockFlag extends BlockLetsModReboot {

	public BlockFlag() {
		super();
		this.setBlockName("flag block");
		this.setCreativeTab(CreativeTabs.tabBlock);
		this.setBlockTextureName("blockFlag");
	}
}
